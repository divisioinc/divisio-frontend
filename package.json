{
  "name": "cloud-ag-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build && cp build/index.html build/404.html",
    "test:watch": "react-app-rewired test",
    "deploy": "npm-run-all --parallel lint prettier build",
    "lint": "eslint --ignore-path .gitignore --ext .js .",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json)\""
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-divisio": "^0.3.4",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react-app-rewire-aliases": "^0.2.0",
    "react-app-rewired": "^2.1.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
